{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "src",
    "paths": {
      "@styles/*":["styles/*"],
      "@components/*":["components/*"],
      "@layout/*":["layout/*"],
      "@common/*":["common/*"],
      "@containers/*":["containers/*"],
      "@templates/*":["templates/*"],
      "@hooks/*":["hooks/*"],
      "@services/*":["services/*"],
      "@logos/*":["assets/logos/*"],
      "@images/*":["assets/images/*"],
      "@icons/*":["assets/icons/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/dashboard/edit/[id].tsx"],
  "exclude": ["node_modules"]
}
